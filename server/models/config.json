{
  "restApiRoot": "/api",
  "host": "0.0.0.0",
  "port": 3000,
  "url": "http://localhost:3000/",
  "remoting": {
    "context": false,
    "rest": {
      "handleErrors": false,
      "normalizeHttpPath": false,
      "xml": false
    },
    "json": {
      "strict": false,
      "limit": "5000000kb"
    },
    "urlencoded": {
      "extended": true,
      "limit": "500000kb"
    },
    "cors": false
  },
  "legacyExplorer": false,
  "logoutSessionsOnSensitiveChanges": true
}
